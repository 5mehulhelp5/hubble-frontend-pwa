{
    "name": "@hubblecommerce/hubble",
    "version": "2.0.0-alpha.26",
    "description": "PWA for eCommerce frameworks based on NuxtJs",
    "author": "hubble commerce",
    "license": "MIT",
    "type": "module",
    "engines": {
        "npm": ">=6.14.15",
        "node": ">=14.18.1"
    },
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        },
        "./commons": {
            "import": "./dist/runtime/commons/index.mjs"
        },
        "./theme/store": {
            "import": "./dist/theme/store/index"
        },
        "./platforms/shopware/api-client": {
            "import": "./dist/platforms/shopware/api-client/generated/index.mjs"
        },
        "./platforms/shopware/api-client/utils": {
            "import": "./dist/platforms/shopware/api-client/utils/index.mjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "bin",
        "dist"
    ],
    "bin": {
        "hubble": "./bin/hubble-cli.js"
    },
    "scripts": {
        "prepack": "nuxt-module-build",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:build:analyze": "nuxi analyze playground",
        "dev:prepare": "rm -rf ./playground/.hubble && nuxt-module-build --stub && nuxi prepare playground && husky install",
        "test": "nuxi prepare __tests__/module/fixture && vitest",
        "coverage": "vitest run --coverage",
        "sw:dev:generate-api": "node bin/hubble-cli.js dev:sw open-api-generate $BASEURL $CLIENT_ID $CLIENT_SECRET && git apply src/runtime/platforms/shopware/api-client/*.patch",
        "sw:dev:patch-api": "git format-patch -1 $COMMIT -o src/platforms/shopware/api-client/",
        "sw:install-plugins": "node bin/hubble-cli.js dev:sw sw-plugins-assets",
        "sw:config-plugins": "node bin/hubble-cli.js dev:sw sw-plugins-config",
        "sw:languages": "node bin/hubble-cli.js dev:sw sw-languages"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint --fix"
    },
    "dependencies": {
        "@heroicons/vue": "^2.0.12",
        "@intlify/nuxt3": "^0.2.4",
        "@nuxt/kit": "^3.0.0-rc.11",
        "@nuxtjs/color-mode": "^3.1.7",
        "@nuxtjs/tailwindcss": "^5.3.3",
        "@pinia/nuxt": "^0.4.1",
        "@vueuse/core": "^9.3.0",
        "@vueuse/nuxt": "^9.3.0",
        "daisyui": "^2.31.0",
        "globby": "^13.1.2",
        "lmify": "^0.3.0",
        "mitt": "^3.0.0",
        "portal-vue": "^3.0.0-beta.0",
        "untyped": "^0.4.7",
        "unzipper": "^0.10.11",
        "sass": "^1.55.0"
    },
    "devDependencies": {
        "@nuxt/module-builder": "latest",
        "@nuxt/schema": "^3.0.0-rc.11",
        "@nuxt/test-utils-edge": "^3.0.0-rc.12-27746784.92ff066",
        "@nuxtjs/eslint-config-typescript": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "eslint": "8.22.0",
        "eslint-plugin-vue": "^9.6.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.0",
        "nuxt": "^3.0.0-rc.11",
        "openapi-typescript-codegen": "^0.23.0",
        "typescript": "^4.8.4",
        "vite-plugin-eslint": "^1.8.1",
        "vitest": "^0.23.4"
    }
}
