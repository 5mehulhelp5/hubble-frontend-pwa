{
    "name": "@hubblecommerce/hubble",
    "version": "2.0.0-alpha.15",
    "description": "PWA for eCommerce frameworks based on NuxtJs",
    "author": "hubble commerce",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        },
        "./commons": {
            "import": "./dist/runtime/commons/index.mjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
        "bin",
        "dist"
    ],
    "bin": {
        "hubble": "./bin/hubble-cli.js"
    },
    "scripts": {
        "prepack": "nuxt-module-build",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:build:analyze": "nuxi analyze playground",
        "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground && husky install",
        "test": "nuxi prepare __tests__/module/fixture && vitest",
        "coverage": "vitest run --coverage",
        "sw:dev:generate-api": "node bin/hubble-cli.js dev:sw open-api-generate $BASEURL $CLIENT_ID $CLIENT_SECRET && git apply src/runtime/platforms/shopware/api-client/*.patch",
        "sw:dev:patch-api": "git format-patch -1 $COMMIT -o src/runtime/platforms/shopware/api-client/"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint --fix"
    },
    "dependencies": {
        "@nuxt/kit": "^3.0.0-rc.3",
        "@pinia/nuxt": "^0.3.0",
        "globby": "^13.1.2",
        "mitt": "^3.0.0"
    },
    "devDependencies": {
        "@nuxt/module-builder": "latest",
        "@nuxt/test-utils-edge": "^3.0.0-rc.3-27567768.c1f728e",
        "@nuxtjs/eslint-config-typescript": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.16.0",
        "eslint-plugin-vue": "^9.1.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.0",
        "nuxt": "^3.0.0-rc.3",
        "openapi-typescript-codegen": "^0.23.0",
        "typescript": "^4.7.2",
        "vite-plugin-eslint": "^1.6.1",
        "vitest": "^0.13.1"
    }
}
